public class CharacterAssassination{
    public static void main (String[] args){
        char ch = 'A';
        System.out.println("A char: " + ch);
    }
}


/*
 Name: Aldrin Meller T. Basalo.

 This program displays a "double" value.

 Date: February 6, 2024
 */

public class DoubleJeopardy{
    public static void main (String[] args){
        double value = 32;
        System.out.println("A double: " + value);
    }
}


/*
 Name: Aldrin Meller T. Basalo.

 This program displays a "exponential" value.

 Date: February 6, 2024
 */

public class ExponentialExplosion{
    public static void main (String[] args){
        double value = 32;
        System.out.println("e to the power value: " + Math.exp( value ) );
    }
}


