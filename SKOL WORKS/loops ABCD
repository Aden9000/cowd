A
/*Displays Odd numbers increasing from 1 to 9*/
#include <stdio.h>

int main ()
{
	int i;
  	for (i = 1; i <= 9; i = i + 2)
  		{
    		printf("%d ", i);
  		}
 	return 0;
}
------------------------------------------------
------------------------------------------------
B
/*Displays Odd numbers decreasing from 9 to 1*/
#include <stdio.h>

int main ()
{
  	int i;
  	for (i = 9; i >= 1; i = i - 2)
  		{
      		printf("%d ", i);
 		}
 	return 0;
}
------------------------------------------------
------------------------------------------------
C
/*Displays numbers increasing by 10*/
#include <stdio.h>

int main ()
{
  	int i;
  	for (i = 10; i <= 100; i = i + 10)
  		{
      		printf("%d,", i);
  		}
  	return 0;
}
------------------------------------------------
------------------------------------------------
D
/*Displays 5 rows with consicutive numbers starting from 1, and the length of each row increasing by 1*/
#include <stdio.h>
int main ()
{
  	int i;
  	int j;
  	for (i = 1; i <= 5; i++)
  		{
      		for (j = 1; j <= i; j++)
          		{
              		printf("%d ", j);
          		}
      		printf("\n");
    	}
  	return 0;
}
